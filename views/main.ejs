<!-- Página Agendamento -->
    <!-- <% include agendamento %>  -->
    <div id="agendamento" data-role="page">
      <div data-role="header" data-theme="b">
        <a href="#menu" class="ui-btn ui-shadow ui-corner-all ui-btn-icon-left ui-icon-back ui-btn-icon-notext" data-rel="back" ></a>
        <h1>Agendamento</h1>
      </div>
      <form data-role="content">
        <div class="ui-block-b" id="topoAgendamento">
          <div class="ui-body ui-body-d">
            <label for="dataAgendamento">Data do agendamento:</label>
            <input type="date" data-clear-btn="false" name="dataAgendamento" id="dataAgendamento" value="" />
            <label for="horaAgendamento">Hora:</label>
            <input type="time" data-clear-btn="false" name="horaAgendamento" id="horaAgendamento" value="">
          </div>
        </div>

        <div class="ui-block-c">
          <div class="ui-body ui-body-d">
            <table data-role="table" id="movie-table-custom" data-mode="reflow" class="ui-body-d ui-shadow table-stripe ui-responsive">
            </table>
          </div>
        </div>
        
        <div class="ui-bar">
            <label for="clienteAgendamento">Nome do Cliente:</label>
            <div class="ui-filterable">
              <input id="clienteAgendamento" data-type="search" placeholder="Nome do Cliente">
            </div>
            <ul id="autocompleteAgendamento" data-role="listview" data-inset="true" data-filter="true" data-input="#clienteAgendamento"></ul>

            <div id="divServAgend" class="ui-filterable ui-field-contain">
              <label for="agendServ">Serviço a ser realizado:</label>
              <input id="agendServ" data-type="search" placeholder="Pesquisa valor do serviço">
            </div>
            <ul id="autocompleteAgendServ" data-role="listview" data-inset="true" data-filter="true" data-input="#agendServ"></ul>
            
            <textarea cols="40" rows="8" name="servicoAgendado" id="servicoAgendado" placeholder="Serviço a ser realizado"></textarea>

            <label for="funcionarioAgendamento">Nome do Funcionário:</label>
            <div class="ui-filterable">
              <input id="funcionarioAgendamento" data-type="search" placeholder="Funcionário">
            </div>
            <ul id="autocompleteFuncionario" data-role="listview" data-inset="true" data-filter="true" data-input="#funcionarioAgendamento"></ul>    
        
            <input id="btnAgend" type="button" value="Confirmar">
          <!-- </div> -->
        </div>
      </form>
    </div>
    
<!-- *********************************** -->

<!-- Página Cadastrar Funcionário -->
    <!-- <% include cadastrarFuncionario %> -->

    <div id="cadastrarFuncionario" data-role="page">
      <div data-role="header" data-theme="b">
        <a href="#menu" class="ui-btn ui-shadow ui-corner-all ui-btn-icon-left ui-icon-back ui-btn-icon-notext" data-rel="back" ></a>
        <h1>Cadastro de Funcionário</h1>
      </div> 

      <form>
        <label for="inputNomeFuncionario">Nome do Funcionário:</label>
        <input type="text" name="inputNomeFuncionario" id="inputNomeFuncionario" placeholder="Nome do Funcionário" value="">
  
        <label for="endereco">Endereço:</label>
        <input type="text" name="endereco" id="enderecoRua" placeholder="Rua" value="">
        <input type="text" name="endereco" id="enderecoNumero" placeholder="Número" value="">

        <div class="tamanhoData">
          <label for="inputFoneFuncionario">Tefefone:</label>
          <input type="text" name="inputFoneFuncionario" id="inputFoneFuncionario" placeholder="Telefone" value="">    
        </div>

        <div class="tamanhoData">
          <label for="dataNascimentoFuncionario">Data de nascimento:</label>
          <input type="date" data-clear-btn="false" name="dataNascimentoFuncionario" id="dataNascimentoFuncionario" value="">  
        </div>

        <label for="inputCpfFuncionario">CPF:</label>
        <input type="text" name="inputCpfFuncionario" id="inputCpfFuncionario" placeholder="CPF" value=""> 

        <fieldset data-role="controlgroup" data-type="horizontal" data-mini="true">
          <legend>Sexo:</legend>
            <input type="radio" name="radioSexo" id="radioMasculino" value="masculino" >
            <label for="radioMasculino">Masculino</label>
            <input type="radio" name="radioSexo" id="radioFeminino" value="feminino">
            <label for="radioFeminino">Feminino</label>
        </fieldset>

        <label for="inputFuncaoFuncionario">Função:</label>
        <input type="text" name="inputFuncaoFuncionario" id="inputFuncaoFuncionario" placeholder="Função" value=""> 

        <div class="tamanhoData">
          <label for="dataAdmissao">Data de Admissão:</label>
          <input type="date" data-clear-btn="false" name="dataAdmissao" id="dataAdmissao" value="">
        </div>

        <label for="cursos">Cursos realizados:</label>
        <textarea cols="40" rows="8" name="cursos" id="cursos" placeholder="Cursos realizados"></textarea>
        
        <label for="escolaridade">Escolaridade:</label>
        <input type="text" name="escolaridade" id="escolaridade" placeholder="Escolaridade" value="">

        <fieldset data-role="controlgroup" data-type="horizontal" data-mini="true">
          <legend>Estado Civil:</legend>
          <input type="radio" name="radioEstadoCivil" id="radioSolteiro" value="solteiro">
          <label for="radioSolteiro">Solteiro</label>
          <input type="radio" name="radioEstadoCivil" id="radioCasado" value="casado">
          <label for="radioCasado">Casado</label>
        </fieldset>
        
        <div class="tamanhoData">
          <label for="salario">Salário:</label>
          <input type="text" name="salario" id="salario" placeholder="Salário" value=""> 

          <label for="login">Login:</label>
          <input type="text" name="login" id="login" placeholder="Login" value="">
          
          <label for="password">Senha:</label>
          <input type="password" name="password" id="password" placeholder="Senha" value="">
        </div>

        <fieldset data-role="controlgroup" data-type="horizontal" data-mini="true">
          <legend>Nível de Permissão:</legend>
          <input type="radio" name="radioPermissao" id="permissaoAdmin" value="administrador">
          <label for="permissaoAdmin">Administrador</label>
          <input type="radio" name="radioPermissao" id="permissaoComum" value="comum">
          <label for="permissaoComum">Comum</label>
        </fieldset>
        

        <input type="button" id="btnCadFunc" value="Confirmar">        
      </form>
    </div>

